@inject ExtensionCollection Collection

@foreach (var c in Collection.Extensions)
{
    foreach (var r in c.Resources)
    {
        if (r.Type == ResourceType.Script)
        {
            @((MarkupString)($"<script type=\"text/javascript\" src=\"{r.Path}\" AcBlog_Extensions_Core_Tag=\"{r.Path}\"></script>"))
        }
    }
}
